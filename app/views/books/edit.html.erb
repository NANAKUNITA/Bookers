<h1>
 <strong>Editing Book</strong>
</h1>

 <%= form_with model:@book do |f| %>
   <h4>Tilte</h4>
    <%= f.text_field :title %>

   <h4>Body</h4>
    <%= f.text_area :body %>

<%= f.submit 'Update Book' %>
<%= link_to "Show", book_path(@book.id) %>
<%= link_to "Back", books_path(@book) %>
 <% end %>
